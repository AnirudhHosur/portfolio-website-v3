{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "includeFiles": ["**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "regions": ["iad1"],
  "installCommand": "pip install -r requirements.txt",
  "buildCommand": "",
  "outputDirectory": ".",
  "env": {
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "GEMINI_API_KEY": "@gemini_api_key"
  }
}